{
    "name": "@stephenpoole/hex-tooltip",
    "version": "1.0.23",
    "description": "Tooltips for Dead by Daylight anywhere on the web.",
    "main": "dist/hex.tooltip.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "start": "webpack --mode=production && yarn types",
        "dev": "webpack --mode=development --watch",
        "types": "tsc -d --emitDeclarationOnly --outDir \"dist\"",
        "test": "nyc ts-mocha --reporter min --require @babel/register \"./test/**/*.ts\"",
        "docs": "typedoc --excludeExternals --exclude \"**/node_modules/**\"  --name \"Dead By Daylight Tooltips\" --out \"docs\" \"src\"",
        "coverage": "nyc --reporter=lcov yarn test",
        "snyk": "snyk protect",
        "deploy": "yarn snyk && yarn start && yarn test && yarn publish --access=public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stephenpoole/hex-tooltip.git"
    },
    "author": "Stephen Poole",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/stephenpoole/hex-tooltip/issues"
    },
    "homepage": "https://github.com/stephenpoole/hex-tooltip#readme",
    "dependencies": {
        "@stephenpoole/deadbydaylight": "^5.3.0-alpha5",
        "@tippyjs/react": "^4.2.5",
        "i18next": "^20.3.2",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.11.0",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/register": "^7.0.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.19",
        "@types/i18next": "^13.0.0",
        "@types/node": "^15.12.4",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "@types/react-i18next": "^8.1.0",
        "@types/styled-components": "^5.1.10",
        "@types/webpack": "^4.4.24",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-const-enum": "^1.0.1",
        "babel-plugin-styled-components": "^1.12.0",
        "chai": "^4.3.4",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "codecov": "^3.8.2",
        "css-loader": "^5.2.6",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^4.0.0",
        "eslint-plugin-import": "^2.16.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "mocha": "^5.2.0",
        "nyc": "^15.1.0",
        "snyk": "^1.639.0",
        "source-map": "^0.7.3",
        "source-map-loader": "^3.0.0",
        "source-map-support": "^0.5.10",
        "style-loader": "^3.0.0",
        "terser-webpack-plugin": "^5.1.4",
        "ts-mocha": "^8.0.0",
        "ts-node": "^10.0.0",
        "typedoc": "^0.21.0",
        "typescript": "^4.3.4",
        "url-loader": "^4.1.1",
